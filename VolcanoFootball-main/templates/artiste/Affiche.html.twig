{% extends 'base-back.html.twig' %}
{% block content %}
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">

                <h4 class="card-title">Liste Des Artiste</h4>
                <table id="datatable-buttons" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                    <thead>
                    <tr>
                        <th>ID</th>
                        <th>ID evenement</th>
                        <th>Nom </th>
                        <th>prenom</th>
                        <th>type </th>
                        <th>age</th>
                        <th>details</th>
                        <th>update</th>
                        <th>delete</th>
                       
                    </tr>
                </thead>
                <tbody>
                    <tr {% for a in artiste %}>
                        <td>{{ a.id }}</td>
                        <td>{{ a.idEvenement }}</td>
                        <td>{{ a.nom }}</td>
                        <td>{{ a.prenom }}</td>
                        <td>{{ a.type }}</td>
                        <td>{{ a.age }}</td>
                        <td><a href>details</a></td>
                        <td><a href="{{ path('UpdateArtiste',{'id':a.id}) }}">Update</a></td>
                        <td><a href="{{ path('deleteArtiste',{'id':a.id}) }}">Delete</a></td>
                       
                         
                      
                    </tr{% endfor %}>
                    <li><a href="http://127.0.0.1:8000/AddArtiste">Add</a></li>
                   

                </tbody>
            </table>
        </div>
    </div>
</div> 
</div> 
{% endblock %}     